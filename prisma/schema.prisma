// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model users {
  id         Int         @id @default(autoincrement())
  email      String      @unique
  name       String
  petshop    petshops?   @relation(fields: [petshop_id], references: [id])
  petshop_id Int
}

model petshops {
  id       Int        @id @default(autoincrement())
  name     String
  cnpj     String     @unique 
  users    users[]
}
